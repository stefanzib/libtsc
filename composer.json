{
    "name": "moniros/libtsc",
    "type": "library",
    "homepage": "https://github.com/tpenaranda/libtsc",
    "description": "A small library to get data from personalized TSC Cards (Tarjeta Sin Contacto) using User ID (Documento Nacional de Identidad) and Card ID.",
    "require": {
        "php": ">=5.3.0",
        "ext-curl": "*",
        "ext-mcrypt": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4"
    },
    "autoload": {
        "psr-0" : {
            "MoniRos\\LibTSC" : "lib"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}

